{
    "name": "andrey-helldar/package-wizard",
    "description": "Helps to initialize a new package project",
    "type": "library",
    "license": "MIT",
    "keywords": ["package", "packages", "initializer", "wizard"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/package-wizard/issues",
        "source": "https://github.com/andrey-helldar/package-wizard"
    },
    "require": {
        "php": "^7.3|^8.0",
        "ext-json": "*",
        "composer-plugin-api": "^2.0",
        "andrey-helldar/support": "^2.6",
        "symfony/console": "^4.0|^5.0"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\PackageWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
