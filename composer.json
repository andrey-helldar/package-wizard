{
    "name": "andrey-helldar/package-wizard",
    "description": "Package Wizard is a composer plugin for creating a new packages using CLI tool.",
    "type": "composer-plugin",
    "license": "MIT",
    "keywords": ["package", "packages", "initializer", "wizard", "composer", "plugin"],
    "authors": [
        {
            "name": "Andrey Helldar",
            "email": "helldar@ai-rus.com"
        }
    ],
    "support": {
        "issues": "https://github.com/andrey-helldar/package-wizard/issues",
        "source": "https://github.com/andrey-helldar/package-wizard"
    },
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "composer-plugin-api": "^2.0",
        "andrey-helldar/support": "^2.8",
        "symfony/console": "^5.2",
        "symfony/var-dumper": "^5.2"
    },
    "require-dev": {
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Helldar\\PackageWizard\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "class": "Helldar\\PackageWizard\\Application"
    }
}
